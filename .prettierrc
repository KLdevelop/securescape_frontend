{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "tabWidth": 4,
  "overrides": [
    {
      "files": ["*.scss", "*.json", "./.prettierrc"],
      "options": {
        "tabWidth": 2
      }
    }
  ],
  "useTabs": false,
  "semi": true,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "requirePragma": false,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "singleAttributePerLine": true
}
